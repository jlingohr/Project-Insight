{
  "WHERE": {
    "AND": [{
      "IS": {
        "rooms_furniture": "*Tables*"
      }
    }, {
      "GT": {
        "rooms_seats": 200
      }
    }]
  },
  "OPTIONS": {
    "COLUMNS": [
      "rooms_shortname",
      "minSeats"
    ],
    "ORDER": {
      "dir": "DOWN",
      "keys": ["minSeats"]
    }
  },
  "TRANSFORMATIONS": {
    "GROUP": ["rooms_shortname"],
    "APPLY": [{
      "minSeats": {
        "MIN": "rooms_seats"
      }
    }]
  }
}