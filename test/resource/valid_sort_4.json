{
  "WHERE": {
    "AND": [{
      "IS": {
        "rooms_furniture": "*Tables*"
      }
    }, {
      "GT": {
        "rooms_seats": 200
      }
    }]
  },
  "OPTIONS": {
    "COLUMNS": [
      "rooms_shortname",
      "avgSeats"
    ],
    "ORDER": "avgSeats"
  },
  "TRANSFORMATIONS": {
    "GROUP": ["rooms_shortname"],
    "APPLY": [{
      "avgSeats": {
        "AVG": "rooms_seats"
      }
    }]
  }
}